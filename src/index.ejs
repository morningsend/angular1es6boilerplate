<!DOCTYPE html>
<html ng-app="todo-app">
  <head>
    <meta charset="UTF-8">
    <title><%= htmlWebpackPlugin.options.title %></title>
    <link href="https://fonts.googleapis.com/css?family=Roboto" rel="stylesheet">
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
  </head>
  <body>
    <div ng-controller="todoController">
      <md-toolbar class="md-default">
        <div class="md-toolbar-tools">
          <md-button class="md-icon-button">
            <md-icon md-font-icon="material-icons">menu</md-icon>
          </md-button>
          <h2 class="md-flex">Translation Console</h2>
        </div>
      </md-toolbar>
      <md-content>
        <section layout="row" layout-sm="column" layout-align="center center" layout-wrap>
          <md-button class="md-raised">Button</md-button>
          <md-button class="md-raised md-primary">Primary</md-button>
          <md-button class="md-flat"><md-icon md-font-icon="material-icons">home</md-icon></md-button>
          <div class="label">Raised</div>
        </section>
      </md-content>
    </div>
    <div ng-view>
    </div>
  </body>
</html>